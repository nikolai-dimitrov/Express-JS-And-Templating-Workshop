<main>
  <h1>Browser</h1>
  <form action="/" method="GET">
    <input type="text" class="search" name="search" placeholder="Search..." value="{{search}}" />
    <input
      type="number"
      name="from"
      class="difficulty"
      value="{{from}}"
      placeholder="Difficulty from..."
    />
    <span>-</span>
    <input
      type="number"
      name="to"
      class="difficulty"
      value="{{to}}"
      placeholder="Difficulty to..."
    />
    <input type="submit" value="search" />
  </form>
  {{#each cubes}}
    {{>cubeTemplate}}
  {{/each}}

</main>